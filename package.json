    "bugs": {
        "url": "https://github.com/muster-mark/mark-fisher-photography/issues"
    },
    "homepage": "https://github.com/muster-mark/mark-fisher-photography#readme",
    "repository": "https://github.com/muster-mark/mark-fisher-photography",
    "scripts": {
        "postinstall": "command -v nvm && nvm use || echo \"Please make sure you are using the version of node specified in .nvmrc\"",
        "add-photos": "node -r ts-node/register scripts/extract_metadata.ts && node -r ts-node/register scripts/build_image_data.ts",
        "build": "cross-env NODE_ENV='production' webpack --config webpack.config.js && npm run build-pages && npm run build-rss",
        "dev": "cross-env NODE_ENV='development' webpack --config webpack.config.js --watch",
        "build-pages": "node -r ts-node/register scripts/build_manual_pages.ts && node -r ts-node/register scripts/build_photo_and_gallery_pages.ts && node -r ts-node/register scripts/build_homepage.ts && node -r ts-node/register scripts/build_sitemap.ts",
        "build-rss": "node -r ts-node/register scripts/build_rss.ts",
        "test": "jest",
        "deploy:staging": "node -r ts-node/register --env-file=.staging.env scripts/deploy.ts",
        "deploy:production": "node -r ts-node/register  --env-file=.production.env scripts/deploy.ts",
        "serve": "node -r ts-node/register scripts/serve.ts",
        "update-browserslist": "pnpm update --save caniuse-lite browserslist"
    },
    "sideEffects": [
        "*.vue"
    ],
    "dependencies": {
        "@appnest/masonry-layout": "^2.2.3",
        "date-season": "0.0.2",
        "dotenv": "^16.4.5",
        "lodash": "^4.17.21",
        "morgan": "^1.10.0",
        "node-exiftool": "^2.3.0",
        "nunjucks": "^3.2.4",
        "nunjucks-date": "^1.5.0",
        "slug": "^9.1.0",
        "vue": "^3.4.38"
    },
    "devDependencies": {
        "@aws-sdk/client-cloudfront": "^3.629.0",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/plugin-transform-runtime": "^7.24.7",
        "@babel/preset-env": "^7.25.4",
        "@jest/globals": "^29.7.0",
        "@types/chroma-js": "^2.4.4",
        "@types/compression": "^1.7.5",
        "@types/escape-html": "^1.0.4",
        "@types/express": "^4.17.21",
        "@types/get-pixels": "^3.3.4",
        "@types/lodash": "^4.17.7",
        "@types/markdown-it": "^14.1.2",
        "@types/morgan": "^1.9.9",
        "@types/mustache": "^4.2.5",
        "@types/nunjucks": "^3.2.6",
        "@types/slug": "^5.0.9",
        "autoprefixer": "^10.4.20",
        "babel-jest": "^29.7.0",
        "babel-loader": "^9.1.3",
        "browserslist": "^4.23.3",
        "chroma-js": "^2.6.0",
        "clean-webpack-plugin": "^4.0.0",
        "colors": "^1.4.0",
        "compression": "^1.7.4",
        "copy-webpack-plugin": "^12.0.2",
        "cross-env": "^7.0.3",
        "css-loader": "^7.1.2",
        "cssnano": "^7.0.5",
        "escape-html": "^1.0.3",
        "express": "^4.19.2",
        "get-pixels": "^3.3.3",
        "get-rgba-palette": "^2.0.1",
        "jest": "^29.7.0",
        "markdown-it": "^14.1.0",
        "mini-css-extract-plugin": "^2.9.1",
        "mustache": "^4.2.0",
        "node": "^22.6.0",
        "postcss-loader": "^8.1.1",
        "postcss-logical": "^8.0.0",
        "prettier": "^3.3.3",
        "sass": "^1.77.8",
        "sass-loader": "^16.0.1",
        "terser-webpack-plugin": "^5.3.10",
        "ts-jest": "^29.2.5",
        "ts-loader": "^9.5.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.5.4",
        "vue-loader": "^17.4.2",
        "webpack": "^5.93.0",
        "webpack-bundle-analyzer": "^4.10.2",
        "webpack-cli": "^5.1.4",
        "webpack-manifest-plugin": "^5.0.0"
    }
}
