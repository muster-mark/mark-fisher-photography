        "terser-webpack-plugin": "^5.2.5",
        "ts-loader": "^9.2.6",
        "ts-node": "^9.1.1",
        "typescript": "^4.5.2",
        "vue-loader": "^15.9.8",
        "vue-loader-plugin": "^1.3.0",
        "webpack": "^5.64.4",
        "webpack-bundle-analyzer": "^4.5.0",
