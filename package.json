    "homepage": "https://github.com/muster-mark/markfisher.local#readme",
    "repository": "https://github.com/muster-mark/markfisher.local",
    "scripts": {
        "postinstall": "command -v nvm && nvm use || echo \"Please make sure you are using the version of node specified in .nvmrc\"",
        "add-photos": "node scripts/extract_metadata.js && node scripts/build_image_data.js",
        "build": "cross-env NODE_ENV='production' webpack --config webpack.config.js && npm run build-pages",
        "dev": "cross-env NODE_ENV='development' webpack --config webpack.config.js && npm run build-pages",
        "build-pages": "node scripts/build_manual_pages.js && node scripts/build_photo_and_gallery_pages.js && node scripts/build_homepage.js && node -r ts-node/register scripts/build_sitemap.ts",
        "test": "jest",
        "deploy": "node -r ts-node/register scripts/deploy.ts",
        "serve": "node -r ts-node/register scripts/serve.ts",
        "ping-sitemap": "node -r ts-node/register scripts/ping_sitemap.ts",
        "update-browserslist": "npm --depth 20 update --save caniuse-lite browserslist"
    },
    "sideEffects": [
        "*.vue"
    ],
    "dependencies": {
        "babel-preset-vue": "^2.0.2",
        "date-season": "0.0.2",
        "dotenv": "^11.0.0",
        "glob-promise": "^5.0.0",
        "junk": "^3.1.0",
        "lodash": "^4.17.21",
        "morgan": "^1.10.0",
        "node-exiftool": "^2.3.0",
        "nunjucks": "^3.2.3",
        "nunjucks-date": "^1.5.0",
        "slug": "^5.3.0",
        "vue": "^2.7.10",
        "vue-masonry": "^0.16.0",
        "vue-template-compiler": "^2.7.10",
        "webpack-cli": "^4.10.0"
    },
    "devDependencies": {
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/plugin-transform-runtime": "^7.19.1",
        "@babel/preset-env": "^7.19.1",
        "@types/express": "^4.17.14",
        "autoprefixer": "^10.4.12",
        "aws-sdk": "^2.1224.0",
        "babel-loader": "^8.2.5",
        "browserslist": "^4.21.4",
        "chroma-js": "^2.4.2",
        "clean-webpack-plugin": "^4.0.0",
        "colors": "^1.4.0",
        "compression": "^1.7.4",
        "copy-webpack-plugin": "^10.2.4",
        "core-js": "^3.25.3",
        "cross-env": "^7.0.3",
        "css-loader": "^6.7.1",
        "express": "^4.18.1",
        "get-pixels": "^3.3.3",
        "get-rgba-palette": "^2.0.1",
        "jest": "^29.0.3",
        "markdown-it": "^13.0.1",
        "mini-css-extract-plugin": "^2.6.1",
        "mustache": "^4.2.0",
        "node": "^16.17.1",
        "postcss-clamp": "^4.1.0",
        "postcss-loader": "^6.2.1",
        "postcss-logical": "^5.0.4",
        "sass": "^1.55.0",
        "sass-loader": "^12.6.0",
        "terser-webpack-plugin": "^5.3.6",
        "ts-loader": "^9.4.1",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.3",
        "vue-loader": "^15.10.0",
        "vue-loader-plugin": "^1.3.0",
        "webpack": "^5.74.0",
        "webpack-bundle-analyzer": "^4.6.1",
        "webpack-manifest-plugin": "^4.1.1"
    }
}
