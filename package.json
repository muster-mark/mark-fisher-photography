        "autoprefixer": "^10.4.14",
        "aws-sdk": "^2.1363.0",
        "babel-jest": "^29.5.0",
        "babel-loader": "^9.1.2",
        "browserslist": "^4.21.5",
        "chroma-js": "^2.4.2",
        "clean-webpack-plugin": "^4.0.0",
        "colors": "^1.4.0",
        "compression": "^1.7.4",
        "copy-webpack-plugin": "^11.0.0",
        "core-js": "^3.30.1",
        "cross-env": "^7.0.3",
        "css-loader": "^6.7.3",
        "escape-html": "^1.0.3",
        "express": "^4.18.2",
        "get-pixels": "^3.3.3",
