    "scripts": {
        "postinstall": "command -v nvm && nvm use || echo \"Please make sure you are using the version of node specified in .nvmrc\"",
        "add-photos": "node scripts/extract_metadata.js && node scripts/build_photo_and_gallery_pages.js && node scripts/build_image_data.js",
        "build": "cross-env NODE_ENV='production' webpack --config webpack.config.js && npm run build-pages",
        "dev": "cross-env NODE_ENV='development' webpack --config webpack.config.js && npm run build-pages",
        "build-pages": "node scripts/build_manual_pages.js && node scripts/build_photo_and_gallery_pages.js && node scripts/build_homepage.js",
        "test": "jest",
        "deploy": "node scripts/deploy.js",
        "serve": "node -r ts-node/register/transpile-only scripts/serve.ts",
        "build-sitemap": "node scripts/build_sitemap.js"
    },
    "sideEffects": [
        "*.vue"
    ],
    "dependencies": {
        "babel-preset-vue": "^2.0.2",
        "date-season": "0.0.2",
        "dotenv": "^8.2.0",
