    "homepage": "https://github.com/muster-mark/mark-fisher-photography#readme",
    "repository": "https://github.com/muster-mark/mark-fisher-photography",
    "scripts": {
        "postinstall": "command -v nvm && nvm use || echo \"Please make sure you are using the version of node specified in .nvmrc\"",
        "add-photos": "node -r ts-node/register scripts/extract_metadata.ts && node -r ts-node/register scripts/build_image_data.ts",
        "build": "cross-env NODE_ENV='production' webpack --config webpack.config.js && npm run build-pages && npm run build-rss",
        "dev": "cross-env NODE_ENV='development' webpack --config webpack.config.js --watch",
        "build-pages": "node -r ts-node/register scripts/build_manual_pages.ts && node -r ts-node/register scripts/build_photo_and_gallery_pages.ts && node -r ts-node/register scripts/build_homepage.ts && node -r ts-node/register scripts/build_sitemap.ts",
        "build-rss": "node -r ts-node/register scripts/build_rss.ts",
        "test": "jest",
        "deploy": "node -r ts-node/register scripts/deploy.ts",
        "serve": "node -r ts-node/register scripts/serve.ts",
        "ping-sitemap": "node -r ts-node/register scripts/ping_sitemap.ts",
        "update-browserslist": "npm --depth 20 update --save caniuse-lite browserslist"
    },
    "sideEffects": [
        "*.vue"
    ],
    "dependencies": {
        "@appnest/masonry-layout": "^2.0.8",
        "babel-preset-vue": "^2.0.2",
        "date-season": "0.0.2",
        "dotenv": "^16.0.2",
        "glob": "^10.2.2",
        "junk": "^3.1.0",
